/* HTML5 DECLARATIONS */

article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section,
dialog {
	display: block
}
audio[controls],
canvas,
video {
	display: inline-block;
	*display: inline;
	zoom: 1
}

/* BASE */

html {
	height: 100%;
	font-size: 100%;
	/* Force scrollbar in non-IE and Remove iOS text size adjust without disabling user zoom */
	overflow-y: scroll;
	-webkit-text-size-adjust: 100%;
	background: url('../images/web-header-bkgrd-lg.jpg') no-repeat center center fixed;
	-webkit-background-size: cover;
	background-size: cover;
}
body {
	margin: 0;
	min-height: 100%;
	-webkit-font-smoothing: antialiased;
	font-smoothing: antialiased;
	text-rendering: optimizeLegibility;
	color: #403a35;
}

/* Improve default text rendering, handling of kerning pairs and ligatures */


/* DEFAULT FONT SETTINGS */


/* 16px base font size with 150% (24px) friendly, unitless line height and margin for vertical rhythm */


/* Font-size percentage is based on 16px browser default size */

body,
button,
input,
select,
textarea {
	font: 100% 'Abel', sans-serif;
	*font-size: 1em;
	/*	color: #36312d*/
	outline:none;
}
textarea {color: #36312d; outline:none}
/* IE7 and older can't resize px based text */

p,
blockquote,
q,
pre,
address,
hr,
code,
samp,
dl,
ol,
ul,
form,
table,
fieldset,
menu,
img {
	margin: 0 0 1.5em;
	padding: 0
}

/* TYPOGRAPHY */


/* Composed to a scale of 12px, 14px, 16px, 18px, 21px, 24px, 36px, 48px, 60px and 72px */

h1,
h2,
h3,
h4,
h5,
h6 {
	font-family: 'Montserrat', sans-serif;
	color: #554d46;
	font-weight: 700
}

/*#403a35*/

h1 {
	margin: 0;
	font-size: 3.75em;
	line-height: 1.2em;
	margin-bottom: 0.4em
}

/* 60px / 72px */

h2 {
	margin: 0;
	font-size: 2.5em;
	line-height: 1em;
	margin-bottom: 0.5em;
	letter-spacing: -1px
}

/* 48px / 48px */

h3 {
	font-family: 'Michroma', serif;
	font-weight: 400;
	margin: 0;
	font-size: 1.8em;
	line-height: 1.33em;
	margin-bottom: 0.6667em
}

/* 36px / 48px */

h4 {
	margin: 0;
	font-size: 1.66em;
	line-height: 1em;
	margin-bottom: .33em;
	font-weight: normal;
	letter-spacing: 2px;
	font-family: 'Stint Ultra Condensed', serif;
	color: #fe530d;
}

/* 24px / 24px */

h5 {
	margin: 0;
	font-size: 1.2em;
	line-height: 1.142em;
	margin-bottom: 1.142em;
	font-weight: 400;
}

/* 21px / 24px */

h6 {
	margin: 0;
	font-size: .9em;
	line-height: 1.33em;
	margin-bottom: 1.333em;
	font-weight: 400;
}

/* 18px / 24px */

p,
ul,
blockquote,
pre,
td,
th {
	margin: 0;
	font-size: 1.1em;
	line-height: 1.42em;
	margin-bottom: .75em
}

/* 16px / 24px */

small,
p.small {
	margin: 0;
	font-size: 0.875em;
	line-height: 1.625em;
	margin-bottom: 1.625em
}

/*1.7142857142857142857142857142857em}  14px / 24px */

p .lead,
p.lead {
	font-size: 1.2em;
	line-height: 1.75em;
	display: block;
	text-shadow: 0 1px 1px
}
p a {
	font-family: 'Montserrat', sans-serif;
}

/* CODE */

pre {
	white-space: pre;
	white-space: pre-wrap;
	word-wrap: break-word
}
strong p, p strong {font-weight: 600; color:#90694E }
/* Allow line wrapping of 'pre' */

pre,
code,
kbd,
samp {
	font-size: 1em;
	line-height: 1.5em;
	margin-bottom: 1.5em;
	font-family: Menlo, Consolas, 'DejaVu Sans Mono', Monaco, monospace
}

/* TABLES */

table {
	border-collapse: collapse;
	border-spacing: 0;
	margin-bottom: 1.5em
}
th {
	text-align: left
}
tr,
th,
td {
	padding-right: 1.5em;
	border-bottom: 0 solid #36312d
}

/* FORMS */

form {
	margin: 0
}
fieldset {
	border: 0;
	padding: 0
}
textarea {
	overflow: auto;
	vertical-align: top
}
legend {
	*margin-left: -.75em
}
button,
input,
select,
textarea {
	vertical-align: baseline;
	*vertical-align: middle
}

/* IE7 and older */

button,
input {
	line-height: normal;
	*overflow: visible
}
button,
input[type="button"],
input[type="reset"],
input[type="submit"] {
	cursor: pointer;
	-webkit-appearance: button
}
input[type="checkbox"],
input[type="radio"] {
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box
}
input[type="search"] {
	-webkit-appearance: textfield;
	-moz-box-sizing: content-box;
	-webkit-box-sizing: content-box;
	box-sizing: content-box
}
textarea {
	-webkit-appearance: textfield;
	-moz-box-sizing: content-box;
	-webkit-box-sizing: content-box;
	box-sizing: content-box
}

input[type="search"]::-webkit-search-decoration {
	-webkit-appearance: none
}
button::-moz-focus-inner,
input::-moz-focus-inner {
	border: 0;
	padding: 0
}
textarea::-moz-focus-inner {
	border: 0;
	padding: 0;
	outline:none;
}

/* QUOTES */

blockquote,
q {
	quotes: none
}
blockquote:before,
blockquote:after,
q:before,
q:after {
	content: '';
	content: none
}
blockquote,
q,
cite {
	font-style: italic
}
blockquote {
	padding-left: 1.5em;
	border-left: 3px solid #26c6c6
}
blockquote > p {
	padding: 0
}

/* LISTS */

ul,
ol {
	list-style-position: inside;
	padding: 0
}
li ul,
li ol {
	margin: 0 1.5em
}
dl dd {
	margin-left: 1.5em
}
dt {
	font-family: "Montserrat", AppleGothic, sans-serif
}

/* HYPERLINKS */

a {
	text-decoration: none;
	color: #fe530d;
	/*#a55f2c*/
	outline: none;
	/*  transition: color 0.5s linear, border 0.5s linear, opacity 0.5s linear, background-color 0.5s linear;*/
	-webkit-transition: all .6s cubic-bezier(.96, -0.29, 1, 1.03);
	transition: all .6s cubic-bezier(.96, -0.29, 1, 1.03);
}
a:hover,
a:focus {
	text-decoration: none;
	border: none;
	color: #f89c41;
	cursor: default;
	-webkit-transition: all .4s cubic-bezier(.07, 1.09, 1, 1.02);
	transition: all .4s cubic-bezier(.07, 1.09, 1, 1.02);
	outline: none;
}

/*#C0392B*/

a:active {
	text-decoration: none;
	border: none;
	color: #a14327;
	outline: none
}

/*#ff530d*/


/* MEDIA */

figure {
	margin: 0
}
img,
embed,
video {
	max-width: 100%;
	_width: 100%
}

/* Fluid images */

img {
	border: 0;
	-ms-interpolation-mode: bicubic
}

/* Improve IE's resizing of images */

svg:not(:root) {
	overflow: hidden
}

/* Correct IE9 overflow */


/* ABBREVIATION */

abbr[title],
dfn[title] {
	border-bottom: 1px dotted #36312d;
	cursor: help
}

/* MARKED/INSERTED/DELETED AND SELECTED TEXT */

ins,
mark {
	text-decoration: none
}
mark {
	background: #c47529
}
ins {
	background: #d49855
}
del {
	text-decoration: line-through
}

/* selected text */

::-moz-selection {
	background: #26c6c6;
	color: #f9f9f7;
	text-shadow: none;
	outline: none;
}

::selection {
	background: #26c6c6;
	color: #f9f9f7;
	text-shadow: none;
	outline:none;
}

/* selected text */


/* OTHERS */

strong,
b,
dt {
	font-weight: bold
}
dfn {
	font-style: italic
}
var,
address {
	font-style: normal
}
sub,
sup {
	font-size: 75%;
	line-height: 0;
	position: relative;
	vertical-align: baseline
}

/* Position 'sub' and 'sup' without affecting line-height */

sup {
	top: -0.5em
}

/* Move superscripted text up */

sub {
	bottom: -0.25em
}

/* Move subscripted text down */

span.amp {
	font-family: Adobe Caslon Pro, Baskerville, "Goudy Old Style", "Palatino", "Palatino Linotype", "Book Antiqua", Georgia, "Times New Roman", Times, serif;
	font-style: italic;
	font-size: 110%;
	line-height: 0;
	position: relative;
	vertical-align: baseline
}

/* Best available ampersand */

.fright {
	float: right
}
.fleft {
	float: left
}

/* MICRO CLEARFIX HACK */

.cf:before,
.cf:after {
	content: "";
	display: table
}

/* For modern browsers */

.cf:after {
	clear: both
}
.cf {
	zoom: 1
}

/* For IE 6/7 (trigger hasLayout) */


