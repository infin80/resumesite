> 1024px            - 3-Column Fluid Grid
Change widths as necessary
------------------------------------------- */
/* MOBILE PORTRAIT */
@media only screen and (min-width: 320px) {
	body {}
	.tab > a {padding-left: 18px; padding-right: 18px}
	.tab > a i {font-size: 16px; margin-right: 0}
	.g4, #contact .g4.form-container {width: 98.0%}
	.scrollbox {max-height:inherit; overflow: visible}
	.item-box {min-width: 300px}
	.ui.segment,.ui.segment:first-child,.ui.segment:last-child {margin: 10px}
	#highlights .g2, #resume .g2 {width: 98%}
	#highlights .g1, #resume .g1 {width: 100%}
	#skill li div {
		-webkit-background-size: cover;
		background-size:cover;
		opacity: .5;
	}
	#galleryList li {
		margin-left: 10px;
		margin-right: 10px;
	}
	#galleryList li img {width: 250px;}
}
/* MOBILE LANDSCAPE */
@media only screen and (min-width: 480px) {
	body {}
	#galleryList li{width:220px;height:190px;}
	#galleryList li img {width:inherit;}
}
/* SMALL TABLET */
@media only screen and (min-width: 600px) {
	body {}
	.etabs > li span {display: none}
	.tab > a i {font-size: 20px; margin-right: 3px}
	.tab > a {padding-left: 35px; padding-right: 35px}

}
/* TABLET/NETBOOK */
@media only screen and (min-width: 768px) {
	body {}
	/* Sidebar */
	.sidebar {
		padding-left: 20px;
		border-left: 1px solid #efe9e1;
	}
	/* COLUMN GRID */
	.g1,.g2,.g3 {
		display: inline;
		float: left
	}
	/* 2 COLUMN GRID */
	/*	.g1 {width: 48.0%}*/
	/*	.g2 {width: 48.0%}*/
	.g3 {width: 98.0%}

	.etabs >li span {display: block}


}
/* LANDSCAPE TABLET/NETBOOK/LAPTOP */
@media only screen and (min-width: 1024px) {
	body {}
	.etabs >li span {
		display: inline;
	}
	/* 3 COLUMN GRID */
	.g1 {width: 20%}
	/*31.333*/
	.g2 {width: 76%;}
	/*75.667*/
	.g3 {width: 98.0%}
	#contact .g4 {width: 52.0%}
	#contact .g4.form-container {width:40%}
	#highlights .g2, #resume .g2 {width: 69%}
	#highlights .g1, #resume .g1 {width: 31%}
	.scrollbox {
		overflow-y: scroll;
		overflow-x: hidden;
		padding: 0 20px 10px 0;
		max-height: 220px;
		margin-bottom: 12px;
		border-bottom: 1px solid #dfd3c7;
	}
	#skill li div {
	-webkit-background-size: contain;
	background-size: contain;
	opacity: .8;
	}
}
@media only screen and (min-width: 1280px) {
	/* DESKTOP */
	body {}
}
/* WIDESCREEN */
/* Increased body size for legibility */
@media only screen and (min-width: 1400px) {
	body {}
	/* 18.5px / 28px */
}
/* PRINT */
@media print {
	* {
		background: transparent !important;
		color: black !important;
		text-shadow: none !important;
		-webkit-filter: none !important;
		filter: none !important;
		-ms-filter: none !important
	}
	/* Black prints faster */
	a,
	a:visited {
		color: #444 !important;
		text-decoration: underline
	}
	a[href]:after {
		content: " (" attr(href) ")"
	}
	abbr[title]:after {
		content: " (" attr(title) ")"
	}
	.ir a:after,
	a[href^="javascript:"]:after,
	a[href^="#"]:after {
		content: ""
	}
	/* Don't print links for images, javascript or internal links */
	pre,
	blockquote {
		border: 1px solid #999;
		page-break-inside: avoid;
	}
	thead {
		display: table-header-group;
	}
	/* Repeat header row at top of each printed page */
	tr,
	img {
		page-break-inside: avoid;
	}
	img {
		max-width: 100% !important;
	}
	@page {
		margin: 0.5cm
	}
	p,
	h2,
	h3 {
		orphans: 3;
		widows: 3
	}
	h2,
	h3 {
		page-break-after: avoid
	}
}