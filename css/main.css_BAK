}

#contact h2 {margin-bottom: 5px;}
#contact h2 + h4 {font-size: 2.1em; letter-spacing: 0; margin-top: 0;}
#contact h2 + p, #contact h4 + p {margin-top: 15px}

.main-links fieldset.consolation p {
	display: block;
	color: #403a35;
	margin: 10px 5px;
	text-indent: 0;
}
.main-links fieldset.consolation small {
	line-height: 1.1em;
	font-size: 11px;
	margin-top: 15px;
}
.main-links fieldset.consolation img {
	margin-bottom: 0;
}
.main-links fieldset.consolation .button {
	margin: 0px auto 20px;
	display: table;
