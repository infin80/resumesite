.print {
	width: 100%;
	-moz-animation: print 1.5s ease-out;
	-webkit-animation: print 1.5s ease-out;
}
.logod {
	width: 99%;
	-moz-animation: logod 2s ease-out;
	-webkit-animation: logod 2s ease-out;
}
.web {
	width: 90%;
	-moz-animation: web 2s ease-out;
	-webkit-animation: web 2s ease-out;
}
.html5 {
	width: 85%;
	-moz-animation: html5 2.5s ease-out;
	-webkit-animation: html5 2.5s ease-out;
}
.frameworks {
	width: 80%;
	-moz-animation: frameworks 2.5s ease-out;
	-webkit-animation: frameworks 2.5s ease-out;
}
.css3 {
	width: 80%;
	-moz-animation: css3 2.5s ease-out;
	-webkit-animation: css3 2.5s ease-out;
}
.javascript {
	width: 50%;
	-moz-animation: javascript 3s ease-out;
	-webkit-animation: javascript 3s ease-out;
}
.uidesigns {
	width: 60%;
	-moz-animation: uidesigns 3s ease-out;
	-webkit-animation: uidesigns 3s ease-out;
}
.mobile {
	width: 55%;
	-moz-animation: mobile 3s ease-out;
	-webkit-animation: mobile 3s ease-out;
}
.marketing {
	width: 80%;
	-moz-animation: marketing 3.5s ease-out;
	-webkit-animation: marketing 3.5s ease-out;
}
.seo {
	width: 70%;
	-moz-animation: seo 3.5s ease-out;
	-webkit-animation: seo 3.5s ease-out;
}
.socialmedia {
	width: 85%;
	-moz-animation: socialmedia 4s ease-out;
	-webkit-animation: socialmedia 4s ease-out;
}
.typography {
	width: 95%;
	-moz-animation: typography 4.5s ease-out;
	-webkit-animation: typography 4.5s ease-out;
}
