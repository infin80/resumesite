#portfolio {
	background: #efe9e1;
	/* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#f9f9f7+0,efe9e1+100 */
	background: #f9f9f7;
	/* Old browsers */
	/* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #f9f9f7), color-stop(100%, #efe9e1));
	/* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #f9f9f7 0%, #efe9e1 100%);
	/* Chrome10+,Safari5.1+ */
	/* Opera 11.10+ */
	/* IE10+ */
	background: -webkit-gradient(linear, left top, left bottom, from(#f9f9f7), to(#efe9e1));
	background: linear-gradient(to bottom, #f9f9f7 0%, #efe9e1 100%);
	/* W3C */
	filter: progid: DXImageTransform.Microsoft.gradient( startColorstr='#f9f9f7', endColorstr='#efe9e1', GradientType=0);
	/* IE6-9 */
	width: 105%;
}

.img-responsive {max-width: 100%; height: auto}

#portfolio .img-responsive {
	margin: 10px 10px 0 0;
	float: right;
	shape-outside: url(../images/web-graphic-logo-services.png);
	shape-image-threshold: .5;

}
#portfolio .g3 {padding: 35px 25px 25px}
.layered-paper {
	background: #f9f9f7;
	-webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.15), /* The top layer shadow */
		0 10px 0 -5px #f9f9f7, /* The second layer */
		0 10px 1px -4px rgba(0, 0, 0, 0.15), /* The second layer shadow */
		0 20px 0 -10px #f9f9f7, /* The third layer */
		0 20px 1px -9px rgba(0, 0, 0, 0.15);
	box-shadow: 0 1px 1px rgba(0, 0, 0, 0.15), /* The top layer shadow */
		0 10px 0 -5px #f9f9f7, /* The second layer */
		0 10px 1px -4px rgba(0, 0, 0, 0.15), /* The second layer shadow */
		0 20px 0 -10px #f9f9f7, /* The third layer */
		0 20px 1px -9px rgba(0, 0, 0, 0.15);
	/* The third layer shadow */
	width: 200px;
	height: 250px;
}
/******   UberGallery   http://www.ubergallery.net   **********/
#galleryList {
	display: inline-block;
	margin: 0;
	padding: 0;
	/* For IE, the outcast */
	zoom: 1;
	*display: inline;
}
#galleryList li {
	float: left;
	list-style: none;
	margin: 18px;
	padding: 2px;
	background-color: rgba(223, 211, 199, .5);
	width: 220px;
	height: 190px;
	border: 5px outset #695e54;
	/*	border-color: #a58e78 #a58e78 #695e54 #695e54;*/
	overflow: hidden;
	/*	box-shadow: 0 3px 9px 1px rgba(42, 39, 35, 0.88);*/
	-webkit-box-shadow: inset 0 4px 3px 2px rgba(42, 39, 35, .9), inset 0 -3px 3px 2px rgba(249, 249, 247, .85), 0px 8px 6px -2px rgba(42, 39, 35, 0.7), 0px 20px 14px -1px rgba(42, 39, 35, 0.3);
	box-shadow: inset 0 4px 3px 2px rgba(42, 39, 35, .9), inset 0 -3px 3px 2px rgba(249, 249, 247, .85), 0px 8px 6px -2px rgba(42, 39, 35, 0.7), 0px 20px 14px -1px rgba(42, 39, 35, 0.3);
	-webkit-transition: all .2s ease-out;
	transition: all .2s ease-out;
}
#galleryList li img {
	-webkit-filter: opacity(95%);
	opacity: .95;
}
#galleryList li a {
	display: block;
	padding: 4px;
}
#galleryList li:hover {
	border: 5px solid #ff8408;
	background-color: #fbfbfb;
	-webkit-box-shadow: 0px 3px 2px -1px rgba(42, 39, 35, 0.8), 0px 6px 4px -3px rgba(42, 39, 35, 0.4);
	box-shadow: 0px 3px 2px -1px rgba(42, 39, 35, 0.8), 0px 6px 4px -3px rgba(42, 39, 35, 0.4);
	-webkit-transform: translateY(-2px);
	-ms-transform: translateY(-2px);
	transform: translateY(-2px)
}
#galleryList li:hover img {
	-webkit-filter: opacity(100%);
	opacity: 1;
}
/* -----| GALLERY FOOTER |--------------- */
#galleryFooter {
	border-top: 1px solid #CCC;
	margin-top: 10px;
	padding: 3px;
}
#credit {
	float: right;
	font-size: 0.9em;
	margin: 4px 0;
}
#galleryFooter #credit a {
	color: #369;
}
/* ----|  PAGINATION  |------------- */
#galleryPagination {
	color: #369;
	float: left;
	font-size: .9em;
	margin: 0 !important;
	padding: 0 !important;
	text-align: center;
}
#galleryPagination li {
	border: 1px solid transparent;
	border-radius: 3px;
	display: block;
	float: left;
	list-style: none;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
}
#galleryPagination li:hover {
	background-color: #EFEFEF;
	border: 1px solid #CCC;
}
#galleryPagination li.title {
	background: transparent url(page_white_stack.png) no-repeat left center;
	border: 1px solid transparent;
	font-weight: normal;
	padding: 3px 6px 3px 22px;
}
#galleryPagination li a,
#galleryPagination li a:visited {
	color: #369;
	display: block;
	padding: 3px 6px;
	text-decoration: none;
}
#galleryPagination li.current {
	background-color: transparent;
	border: 1px solid transparent;
	color: #36312d;
	font-weight: bold;
	padding: 3px 6px;
}
#galleryPagination li.inactive {
	background-color: transparent;
	color: #ACACAC;
	border: 1px solid transparent;
	display: block;
	padding: 3px 6px;
}
