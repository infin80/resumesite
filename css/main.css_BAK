.atable {display: table}
.atable img {
	max-width: 45%;
	height: auto;
	display: inline-block;
	vertical-align: text-top;
}
.atable ol {
	margin: 0 0 0 8px;
	display: inline-block;
	width: 50%;
	vertical-align: text-top;
}
.atable ol > li {
	list-style-type: none;
	margin: 0 0 7px 5px;
	padding: 3px;
