#toTop {
	bottom: 50px;
	right: 50px;
	display: none;
	width: 30px;
	height: 30px;
	overflow: hidden;
	position: fixed;
	text-decoration: none;
	z-index: 9999;

	font-size: 0;
	color: rgba(255, 255, 255, 0);
	background-color: #ff8408;
	border-radius: 3px;
}
#toTop:after {
	color: #fbfbfb;
	content: '\f077';
	font-size: 18px;
	line-height: 30px;
	font-family: 'FontAwesome';
	text-align: center;
	display: block;
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	-webkit-transition: all 0.2s linear;
	transition: all 0.2s linear;
}
#toTopHover, #toTop:hover {
	background-color: rgba(42, 39, 35, .8);
	bottom: 47px;
}

h4 {
	margin: 0;
	font-size: 1.66em;
	line-height: 1em;
	margin-bottom: .33em;
	font-weight: normal;
	letter-spacing: 2px;
	font-family: 'Stint Ultra Condensed', serif;
	color: #fe530d;
}
